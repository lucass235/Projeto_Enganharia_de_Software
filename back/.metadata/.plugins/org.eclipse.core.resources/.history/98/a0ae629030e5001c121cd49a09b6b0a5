package com.losporocas.estagiagil.dto;

public class CoordenadorDTO extends PessoaDTO{

	private String area;

	public CoordenadorDTO {
		super();
	}
	
	public CoordenadorDTO(Long id, String matricula, String nome, String email, int periodo, boolean estagiando) {
		super(id, matricula, nome, email);
		this.periodo = periodo;
		this.estagiando = estagiando;
	}
	
}
