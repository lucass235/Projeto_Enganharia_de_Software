package com.losporocas.estagiagil.dto;

import javax.validation.constraints.NotEmpty;

public class NewCoordenadorDTO extends NewPessoaDTO{

	@NotEmpty
	private String area;
	
	public NewCoordenadorDTO(String matricula, String nome, String email, String senha, String area) {
		super(matricula, nome, email, senha);
		// TODO Auto-generated constructor stub
		this.area = area;
	}

	public String getArea() {
		return area;
	}

	public void setArea(String area) {
		this.area = area;
	}
	
	
}
